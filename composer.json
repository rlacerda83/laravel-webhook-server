{
    "name": "rlacerda83/laravel-webhook-server",
    "description": "Send webhooks in Laravel apps",
    "keywords": [
        "spatie",
        "laravel-webhook-server",
        "webhook",
        "server"
    ],
    "homepage": "https://github.com/rlacerda83/laravel-webhook-server",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/bus": "^5.8|^6.0",
        "illuminate/queue": "^5.8|^6.0",
        "illuminate/support": "^5.8|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\WebhookServer\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\WebhookServer\\WebhookServerServiceProvider"
            ]
        }
    }
}
